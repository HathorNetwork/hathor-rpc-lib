    "$root$": {
      "globals": {
        "console": true
      }
    },
    "@hathor/snap-utils": {
      "globals": {
        "console": true
      }
    },
    "@hathor/wallet-lib>lodash": {
      "globals": {
        "clearTimeout": true,
        "setTimeout": true
      }
    },
    "assert>call-bind>get-intrinsic": {
